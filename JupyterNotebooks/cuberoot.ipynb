    "#n = float(input('Enter a number to get its cube root:'))\n",
    "n = 512\n",
    "x = n\n",
    "y = (2*x + n/ x**x)/3\n",
    "while abs(x-y) >= 0.0000001:\n",
    "    x=y\n",
    "    y=(2*x + n/ x**2)/3\n",
    "cubeRoot = y\n",
    "print(cubeRoot)"
